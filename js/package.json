{
  "name": "filemanager",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^3.26.7",
    "axios": "^0.19.2",
    "i18next": "^17.3.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "react-scripts": "3.1.1",
    "babel-eslint": "10.0.2",
    "eslint": "6.1.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "prettier": "1.18.2",
    "pretty-quick": "1.11.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "lint": "eslint --fix --ext .js --ext .jsx src",
    "build": "react-scripts build && cp -r build/* ../public",
    "clean": "rm -rf ../public/static && rm ../public/*.js ../public/*.json ../public/index.html",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
